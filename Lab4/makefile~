all: program

program: obj/main.o obj/list_head.o obj/element.o obj/timer.o
	g++ -Iinc -Wall -pedantic -o program obj/main.o obj/list_head.o obj/element.o obj/timer.o -o program

obj/main.o: src/main.cpp
	g++ -c -Iinc -Wall -pedantic -o obj/main.o src/main.cpp 

obj/list_head.o: src/list_head.cpp 
	g++ -c -Iinc -Wall -pedantic -o obj/list_head.o src/list_head.cpp 

obj/timer.o: src/timer.cpp
	g++ -c -Iinc -Wall -pedantic -o obj/timer.o src/timer.cpp

obj/element.o: src/element.cpp
	g++ -c -Iinc -Wall -pedantic -o obj/element.o src/element.cpp


clean:
	rm -f obj/*.o
